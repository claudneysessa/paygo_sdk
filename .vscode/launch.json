{

  "version": "0.2.0",

  "configurations": [

      {
          "name": "paygo_sdk",
          "request": "launch",
          "type": "dart"
      },

      {
          "name": "paygo_sdk (profile mode)",
          "request": "launch",
          "type": "dart",
          "flutterMode": "profile"
      },

      {
          "name": "paygo_sdk (release mode)",
          "request": "launch",
          "type": "dart",
          "flutterMode": "release"
      }

  ],

  "dart.flutterSdkPaths": ["/Users/$USER/fvm/versions"],

  // Remove .fvm files from search
  "search.exclude": {
      "**/.fvm": true
  },

  // Remove from file watching
  "files.watcherExclude": {
      "**/.fvm": true
  }

}